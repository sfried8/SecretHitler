    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "include": [
        "./src/**/*"
    ]
}